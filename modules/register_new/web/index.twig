<div id="register-new-backdrop">
	<div id="register-new">
		<a id="register-new-close" href="javascript:void(0);" data-keybind="esc">Close</a>

		<h2>New report</h2>
		<hr>
		<form method="post" action="/register/new/">
			<div class="date">
				<div class="date-from">
					<input type="text" id="register-new-date-from" name="date-from" value="{{ date_from }}" tabindex="1">
				</div>
				<div class="date-to">
					<input type="text" id="register-new-date-to" name="date-to" tabindex="2">
				</div>
			</div>

			<div class="source">
				<input type="text" name="source" tabindex="3">
			</div>

			<div class="type">
				<input type="text" name="type" tabindex="4">
			</div>

			<div class="customer">
				<input type="text" name="customer" tabindex="5">
			</div>

			<div class="customer_user">
				<input type="text" name="customer_text" tabindex="6">
			</div>

			<div class="location">
				<input type="text" name="location" tabindex="7">
			</div>

			<div class="ticket">
				<input type="text" name="ticket" tabindex="8">
			</div>

			<div class="servers">
				<input type="text" name="servers" tabindex="9">
			</div>
		</form>
	</div>
</div>