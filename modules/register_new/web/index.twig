<div id="register-new">
	<h1>New report</h1>
	<hr>
	<form method="post" action="/register/new/">
		<div class="date">
			<div class="date-from">
				<input type="date" name="date-from">
			</div>
			<div class="date-to">
				<input type="date" name="date-to">
			</div>
		</div>
		<div class="source">
			<input type="text" name="source">
		</div>

		<div class="type">
			<input type="text" name="type">
		</div>

		<div class="customer">
			<input type="text" name="customer">
		</div>

		<div class="customer_user">
			<input type="text" name="customer_text">
		</div>

		<div class="location">
			<input type="text" name="location">
		</div>

		<div class="ticket">
			<input type="text" name="ticket">
		</div>

		<div class="servers">
			<input type="text" name="servers">
		</div>
	</form>
</div>