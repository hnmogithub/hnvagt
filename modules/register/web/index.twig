<div id="register-content">
{% for report in reports %}
	<a class="report">
		<div class="date">
			<div class="date-from">
				{{ report.from }}
			</div>
			<div class="date-to">
				{{ report.to }}
			</div>
		</div>

		<div class="source">
			{{ report.source () }}
		</div>

		<div class="type">
			{{ report.type () }}
		</div>

		<div class="customer">
			{{ report.customer ().name }}
		</div>

		<div class="customer_user">
			{{ report.customerUser () }}
		</div>

		<div class="location">
			{{ report.location }}
		</div>

		<div class="ticket">
			{{ report.ticket }}
		</div>

		<div class="servers">
			{{ report.servers () }}
		</div>

		<div class="description">
			{{ report.description }}
		</div>
	</a>
{% endfor %}
</div>